{
  "name": "javascript-test-boilerplate",
  "private": "true",
  "version": "1.0.0",
  "description": "Kickstart a testing project with Meteor, React and react-router",
  "main": "src/server.js",
  "browser": "src/client.js",
  "scripts": {
    "start": "meteor --settings settings.json",
    "chimp": "./node_modules/.bin/chimp --ddp=http://localhost:3000 --watch --mocha --path=tests",
    "test": ".scripts/test.js",
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "dependencies": {
    "less": "^2.3.1",
    "react": "^15.0.2",
    "react-addons-pure-render-mixin": "^15.0.2",
    "react-addons-update": "^15.1.0",
    "react-dom": "^15.0.2",
    "react-mixin": "^3.0.5",
    "react-router": "^2.4.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chimp": "^0.34.1",
    "css-loader": "^0.23.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-meteor": "^3.6.0",
    "eslint-plugin-react": "^5.2.2",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "less": "^2.3.1",
    "less-loader": "^2.2.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "eslintConfig": {
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended"
    ],
    "rules": {}
  }
}
